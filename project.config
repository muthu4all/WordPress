[project]
	description = WordPress, Git-ified. Synced via SVN every 15 minutes, including branches and tags! This repository is just a mirror of the WordPress subversion repository. Please do not send pull requests. Submit patches to http://core.trac.wordpress.org/ instead.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/muthu4all (muthu4all)
[access "refs/for/refs/*"]
	read = group user/muthu4all (muthu4all)
	push = group user/muthu4all (muthu4all)
	label-Code-Review = -2..+2 group user/muthu4all (muthu4all)
	label-Verified = -1..+1 group user/muthu4all (muthu4all)
	rebase = group user/muthu4all (muthu4all)
	removeReviewer = group user/muthu4all (muthu4all)
	submit = group user/muthu4all (muthu4all)
[access "refs/heads/*"]
	read = group user/muthu4all (muthu4all)
	create = group user/muthu4all (muthu4all)
	push = +force group user/muthu4all (muthu4all)
	pushMerge = group user/muthu4all (muthu4all)
[access "refs/tags/*"]
	push = +force group user/muthu4all (muthu4all)
	pushTag = group user/muthu4all (muthu4all)
	pushSignedTag = group user/muthu4all (muthu4all)
